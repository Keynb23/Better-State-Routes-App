{
  "expo": {
    "scheme": "BetterRoutes",
    "name": "Better-Routes",
    "slug": "Better-Routes",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.betterstatellc.routesapp",
      "config": {
        "googleServicesFile": "./assets/GoogleService-Info.plist"
      }
    },
    
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.betterstatellc.routesapp",
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./assets/google-services.json"
    },
    "web": {},
    "plugins": [
      "expo-router"
    ]
  }
}